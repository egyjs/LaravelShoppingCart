{
    "name": "el3zahaby/laravel-shopping-cart",
    "description": "Laravel Shopping Cart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "A.El-zahaby",
            "email": "el3zahaby@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|| 5.6.* || 5.7.* || 7.8.*",
        "illuminate/session": "5.5.*|| 5.6.* || 5.7.* || 7.8.*",
        "illuminate/events":  "5.5.*|| 5.6.* || 5.7.* || 7.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "El3zahaby\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "El3zahaby\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "el3zahaby/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "El3zahaby\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "El3zahaby\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
